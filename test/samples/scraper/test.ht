fun main(path) {
    return {
        'data': [fetchHtml(path)],
        'target': 'parse',
    };
}

fun parse(html) {
    var dom = parseHtml(html)
    return [getText(querySelector(dom, '#main-tag')).trim(), getText(querySelector(dom, 'body p')).trim()]
}