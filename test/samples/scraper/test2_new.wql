html = getRequest(path).parse();
html.querySelectorAll(s'.item')[].eval {
    _text = *.text();
    _description = *.querySelector(s'.description').text();

    if (_text.contains(s'Title 1')) {
        first = _description;
    }

    if (_text.contains(s'Title 2')) {
        second = _description;
    }

    if (_text.contains(s'Title 3')) {
        third = _description;
    }
}
return = merge(first, second, third);